{
	"ArtifactsDest": "builds/",
        "AppName": "catalyze",
        "OutPath": "{{.Dest}}{{.PS}}{{.Version}}{{.PS}}{{.Os}}-{{.Arch}}{{.PS}}{{.ExeName}}{{.Ext}}",
	"Tasks": [
		"default",
		"publish-github"
	],
	"TasksExclude": [
		"rmbin"
	],
	"BuildConstraints": "linux windows darwin",
	"ResourcesExclude": "README*",
	"PackageVersion": "3.0.0",
	"TaskSettings": {
		"publish-github": {
			"body": "Catalyze CLI",
			"owner": "catalyzeio",
			"repository": "cli"
		},
		"tag": {
			"prefix": ""
		}
	},
	"ConfigVersion": "0.9"
}
